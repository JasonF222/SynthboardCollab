@model SoundFile
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Dashboard</title>
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Calligraffitti&family=Permanent+Marker&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="~/css/dashboard.css">
    
</head>
<body>
    

@* make this go bye bye *@
<div id="PlayBackArray">@Model.KeyPath</div>
<button onclick="playBack(timeLine)">Play</button>
<button onclick="selectStyle('triangle')">Classic</button>
<button onclick="selectStyle('sawtooth')">Fire</button>
<button onclick="selectStyle('sine')">Water</button>
<button onclick="selectStyle('square')">Space</button>

<p><a asp-action="DeleteRecording" asp-controller="User" asp-route-id="@Model.SoundFileID" >delete</a></p>
<script src="~/js/playbackscript.js"></script>
</body>
